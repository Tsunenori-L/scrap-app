{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "no-console": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "no-await-in-loop": "off",
    "no-plusplus":"off",
    "react/no-unescaped-entities": "off",
    "quotes": "off"
  }
}